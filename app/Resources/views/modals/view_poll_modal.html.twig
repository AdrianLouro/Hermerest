{% extends 'modals/modal.html.twig' %}

 {% block modal_header %}
     Fecha celebración Carnaval (12/12/2016) - Hasta 15/01/2017
 {% endblock %}

{% block modal_body %}
    <div class="modal-body_left_panel">
        <div class="attachedFilesDiv">
            <img class="icon" src="{{ asset('images/attachment.png') }}" alt="Adjuntar archivo"/> No hay archivos adjuntos.
        </div>
        <textarea class="messageTextArea" id="pollContentTextArea" type="text" rows="20" cols="60" readonly>Será muy divertido.</textarea>
    </div>
    <div class="modal-body_right_panel">
        <b>Resultados</b>
        <br/>
        <ul id="pollResultList">
            <li>Lunes: 105</li>
            <li>Viernes: 205</li>
        </ul>
    </div>

{% endblock %}

{% block modal_footer %}
    <button class="defaultButton" type="button" onclick="closeModal()">Cerrar</button>
{% endblock %}